<div class="container">
<h1>Funcionarios</h1>


<table class="table table-hover">
    <tr class="active"> 
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Cargo</th>
   <!--  <th>Departamento</th> -->
    <th>Interno</th>
    <th>Correo</th>
    <th>Cumplea√±o</th>
  </tr>
  

<% @funcionarios.each do |funcionario| %>
  <tr>
    <td><%= funcionario.nombre %></td>
    <td><%= funcionario.apellido %></td>
    <td><%= funcionario.cargo %></td>
   <!-- <td><%= funcionario.departamento_id %></td>   -->
    <td><%= funcionario.interno %></td>
    <td><%= funcionario.correo %></td>
    <td><%= funcionario.cumpleanio %></td>
    <td><%= link_to 'Show', funcionario %></td>
    <td><%= link_to 'Edit', edit_funcionario_path(funcionario) %></td>
    <td><%= link_to 'Destroy', funcionario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
</div>
